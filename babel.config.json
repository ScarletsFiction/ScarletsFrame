{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-optional-chaining"
  ],
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "chrome": "55",
          "firefox": "42"
        },
        "corejs" : {
          "version" : "3",
          "proposals" : true
        },
        "useBuiltIns": "entry",
        "modules": false,
        "loose": false,
        "shippedProposals": true
      }
    ]
  ]
}