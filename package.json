{
  "name": "scarletsframe",
  "description": "A frontend framework that can help you write a simple web structure",
  "version": "0.33.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/ScarletsFiction/ScarletsFrame.git"
  },
